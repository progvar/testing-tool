{
  "name": "testing-tool",
  "version": "1.0.0",
  "description": "Integration testing tool for REST APIs.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "NODE_ENV=test TZ=utc mocha --require co-mocha --timeout 30000 test/test.js",
    "test-windows": "set NODE_ENV=test& TZ=utc& mocha --require co-mocha --timeout 60000 test/test.js",
    "coverage": "NODE_ENV=test& TZ=utc& istanbul cover node_modules/mocha/bin/_mocha -- --require co-mocha test/test.js",
    "lint": "eslint src/index.js src/testRunner.js test/test.js config/config.js"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "dependencies": {
    "chai": "3.4.1",
    "co-busboy": "1.3.1",
    "co-mocha": "1.1.2",
    "co-view": "0.0.1",
    "koa": "1.2.0",
    "koa-bodyparser": "2.0.1",
    "koa-router": "5.4.0",
    "koa-static": "2.0.0",
    "mocha": "2.3.4",
    "superagent-proxy": "1.0.0",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "winston": "2.2.0",
    "ws": "1.0.1"
  },
  "devDependencies": {
    "eslint": "1.10.3",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-standard": "1.3.1",
    "istanbul": "0.4.2",
    "pre-push": "0.1.1"
  },
  "engines": {
    "node": "4.2.4"
  }
}
